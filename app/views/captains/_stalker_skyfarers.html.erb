<table class="table table-dark table-hover">
  <thead>
    <tr>
      <th>Name</th>
      <th>Race</th>
      <th>Profession</th>
      <th>State</th>
    </tr>
  </thead>

  <tbody>
    <% @skyfarers.each do |skyfarer| %>
      <tr>
        <td><%= skyfarer.name %></td>
        <td><%= skyfarer.race %></td>
        <td><%= skyfarer.profession %></td>
        <td><%= skyfarer.state %></td>
      </tr>
    <% end %>
  </tbody>
</table>
